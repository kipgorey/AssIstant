{
    "manifest_version": 2,
    "name": "AssIstant",
    "description": "A simple extension to help improve browsing experience.",
    "version": "1.0",
    "browser_action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "tabs",  
      "activeTab",
      "http://localhost:5000/*",
    "http://127.0.0.1:5000/*"
    ]
  }